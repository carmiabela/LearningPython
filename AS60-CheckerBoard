n = int(input("enter row n of n*m"))
m = int(input("enter colomn m of n*m"))

def even(n,m):
  for i in range(n):
    print("")
    for b in range(m):
      print(". * ",end="")
      
def odd(n,m):
  for i in range(n):
    print("")
    for b in range(m//2):
      print(". * ",end="")
    print(" . ")
      

if m%2 !=1:
  even(n,m)
else:
  odd(n,m)
